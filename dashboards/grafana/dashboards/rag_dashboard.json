{
  "dashboard": {
    "title": "RAG Pipeline Observability",
    "tags": ["rag", "observability"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Request Latency by Stage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(rag_request_latency_seconds_bucket[5m]))",
            "legendFormat": "p95 - {{stage}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, rate(rag_request_latency_seconds_bucket[5m]))",
            "legendFormat": "p99 - {{stage}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency (seconds)"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Token Usage by Provider",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "rate(rag_token_usage_total[5m])",
            "legendFormat": "{{provider}} - {{type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Tokens/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Cost per Query",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rag_cost_per_query",
            "legendFormat": "{{provider}} - {{model}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "currencyUSD", "label": "Cost (USD)"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Cache Hit Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "rag_cache_hit_rate",
            "legendFormat": "{{cache_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Hit Rate", "max": 1, "min": 0},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Precision at K",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "histogram_quantile(0.5, rate(rag_precision_at_k_bucket[5m]))",
            "legendFormat": "p50 - k={{k}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(rag_precision_at_k_bucket[5m]))",
            "legendFormat": "p95 - k={{k}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Precision", "max": 1, "min": 0},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Error Rate by Stage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "rate(rag_errors_total[5m])",
            "legendFormat": "{{stage}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Errors/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Total Documents",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "rag_documents_total",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 8,
        "title": "Average Latency (p95)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 24},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(rag_request_latency_seconds_bucket[5m]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "unit": "s"
        }
      },
      {
        "id": 9,
        "title": "Total Token Usage",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "sum(rate(rag_token_usage_total[5m]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 10,
        "title": "Total Errors",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 24},
        "targets": [
          {
            "expr": "sum(rate(rag_errors_total[5m]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  }
}

